          <position x="2" y="5"/>
          <size cx="73" cy="1"/>
        </area>
      </field>
      <field field_type="fixed_text" value="Wollen Sie eine Position nachträglich zur Gutschrift freigeben, müssen   " display_length="73" data_length="73">
