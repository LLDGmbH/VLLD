      <field field_type="fixed_text" value="Hier wird der Zusammenhang zwischen der Remissionsquote (in % auf der X-Achse) und der Ausverkaufsquote     " display_length="108" data_length="108">
        <area>
          <position x="3" y="2"/>
          <size cx="108" cy="1"/>
        </area>
      </field>
      <field field_type="fixed_text" value="(in % auf der Y-Achse) graphisch dargestellt.                                                               " display_length="108" data_length="108">
        <area>
          <position x="3" y="3"/>
          <size cx="108" cy="1"/>
        </area>
      </field>
      <field field_type="fixed_text" value="Man kann zwischen einer Darstellung von nur einem Objekt und der von zwei Objekten wählen.                  " display_length="108" data_length="108">
        <area>
          <position x="3" y="5"/>
          <size cx="108" cy="1"/>
        </area>
      </field>
      <field field_type="fixed_text" value="Außerdem kann man den dargestellten Zeitraum (nach EVT) eingeben. Werden Datumsfelder gelöscht, werden sie  " display_length="108" data_length="108">
        <area>
          <position x="3" y="6"/>
          <size cx="108" cy="1"/>
        </area>
      </field>
      <field field_type="fixed_text" value="durch Unterlassungswerte gefüllt. Bei Überlappungen wird der obere Zeitraum zuerst berücksichtigt.          " display_length="108" data_length="108">
        <area>
          <position x="3" y="7"/>
          <size cx="108" cy="1"/>
        </area>
      </field>
      <field field_type="fixed_text" value="Des weiteren kann der Prozentsatz der Fälle variert werden, um untypische Fälle zu entfernen.               " display_length="108" data_length="108">
        <area>
          <position x="3" y="8"/>
          <size cx="108" cy="1"/>
        </area>
      </field>
      <field field_type="fixed_text" value="Ein Stern (*) hinter der Objektbezeichnung bedeutet, daß hier die Relation zwischen Remissions- und AV-Quote" display_length="108" data_length="108">
        <area>
          <position x="3" y="9"/>
          <size cx="108" cy="1"/>
        </area>
      </field>
      <field field_type="fixed_text" value="günstiger ist.                                                                                              " display_length="108" data_length="108">
        <area>
          <position x="3" y="10"/>
          <size cx="108" cy="1"/>
        </area>
      </field>
      <field field_type="fixed_text" value="Die Schrittweite der Werte auf den Achsen wird durch die jeweils höchsten bzw. niedrigsten Werte bestimmt.  " display_length="108" data_length="108">
        <area>
          <position x="3" y="11"/>
          <size cx="108" cy="1"/>
        </area>
      </field>
      <field field_type="fixed_text" value="Befehlstasten:  F3=Ende = zurück zur Darstellung in Zahlen                                                  " display_length="108" data_length="108">
        <area>
          <position x="3" y="12"/>
          <size cx="108" cy="1"/>
        </area>
      </field>
      <field field_type="fixed_text" value="                F11/12  = nächstes Objekt gleicher Verlag (bei F12 nur Objekte ab einer Mindestgröße)       " display_length="108" data_length="108">
        <area>
          <position x="3" y="13"/>
          <size cx="108" cy="1"/>
        </area>
      </field>
      <field field_type="fixed_text" value="                F10 wie F12, aber das vorherige Objekt wechselt nach unten. (Bitte Zeiträume beachten)      " display_length="108" data_length="108">
